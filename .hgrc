[ui]
username = Jonathan Almeida [:jonalmeida] <jonalmeida942@gmail.com>
merge = internal:merge
editor = vim

[alias]
a = add
b = bookmarks
c = commit
ca = commit --amend
d = diff
ds = diff --stat
he = histedit
l = log -G
lf = log -fr "ancestors(.) & draft()"
lfp = log -fr "last(draft()^ & not draft())" -T "{rev}"
l1 = log -pl 1 -r .
lp = log -Gr "draft() + draft()^"
rb = rebase
pl = pull
ps = push
try = push-to-try -m "try: -b o -p android-api-9,android-api-11 -u all -t none"
try-robo = push-to-try -m "try: -b do -p android-api-9,android-api-11,android-x86 -u robocop -t none"
u = update
i = incoming -n -l1
reset-soft = strip --keep -r .

[extensions]
trychooser = ~/.hgplugins/trychooser
hgwatchman = ~/.hgplugins/hgwatchman/hgwatchman
#hg-experimental = ~/.hgplugins/hg-experimental
histedit = 
hg-prompt = ~/.hgplugins/hg-prompt/prompt.py
record = 
progress = 
pager = 
color = 
rebase = 
mq = 
bzexport = /Users/jalmeida/.mozbuild/version-control-tools/hgext/bzexport
reviewboard = /Users/jalmeida/.mozbuild/version-control-tools/hgext/reviewboard/client.py
bzpost = /Users/jalmeida/.mozbuild/version-control-tools/hgext/bzpost
firefoxtree = /Users/jalmeida/.mozbuild/version-control-tools/hgext/firefoxtree
bundleclone = /Users/jalmeida/.mozbuild/version-control-tools/hgext/bundleclone
push-to-try = /Users/jalmeida/.mozbuild/version-control-tools/hgext/push-to-try
mqext = /Users/jalmeida/.mozbuild/version-control-tools/hgext/mqext
qimportbz = /Users/jalmeida/.mozbuild/version-control-tools/hgext/qimportbz

[color]
